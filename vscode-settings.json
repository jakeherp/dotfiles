{
  "cSpell.language": "en-GB, en-US, de-DE",
  "cSpell.userWords": [
    "autofetch",
    "contentful",
    "davinci",
    "dianomi",
    "dont",
    "esbenp",
    "Figma",
    "Fira",
    "fullhuman",
    "Herper",
    "Hygraph",
    "INFOGRAPHIC",
    "Infographics",
    "instana",
    "Jira",
    "laquo",
    "linkedin",
    "Modernizr",
    "Mstack",
    "netlify",
    "nextjs",
    "openai",
    "pageview",
    "Parens",
    "Permutive",
    "Phonebook",
    "Photoshop",
    "pnpm",
    "Powerline",
    "Prefetch",
    "pubspec",
    "purgecss",
    "quokka",
    "roundtable",
    "snyk",
    "softskill",
    "sourcepoint",
    "standfirst",
    "tailwindcss",
    "testid",
    "theming",
    "tiempos",
    "typegen",
    "vsicons",
    "Zeplin"
  ],
  "css.validate": false,
  "diffEditor.ignoreTrimWhitespace": false,
  "editor.accessibilitySupport": "off",
  "editor.bracketPairColorization.enabled": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": "explicit"
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontFamily": "'Operator Mono', Monaco, 'Courier New', monospace",
  "editor.fontSize": 14,
  "editor.formatOnPaste": true,
  "editor.formatOnSave": true,
  "editor.guides.bracketPairs": "active",
  "editor.inlineSuggest.enabled": true,
  "editor.linkedEditing": true,
  "editor.stickyScroll.enabled": true,
  "editor.tabSize": 2,
  "eslint.lintTask.enable": true,
  "eslint.packageManager": "pnpm",
  "explorer.fileNesting.enabled": true,
  "explorer.fileNesting.patterns": {
    "*.cts": "${capture}.typegen.ts, ${capture}.typegen.ts",
    "*.dart": "${capture}.g.dart",
    "*.mts": "${capture}.typegen.ts, ${capture}.typegen.ts",
    "*.ts": "${capture}.typegen.ts, ${capture}.typegen.ts",
    "*.tsx": "${capture}.typegen.ts, ${capture}.typegen.ts",
    "pubspec.yaml": "pubspec.lock,pubspec_overrides.yaml,.packages,.flutter-plugins,.flutter-plugins-dependencies,.metadata"
  },
  "files.associations": {
    "*.jsonp": "json"
  },
  "git.autofetch": true,
  "git.confirmSync": false,
  "github.copilot.enable": {
    "*": true,
    "yaml": true,
    "plaintext": true,
    "markdown": false,
    "scminput": false
  },
  "gitlens.defaultDateFormat": "Do MMMM YYYY - HH:mm",
  "gitlens.defaultDateShortFormat": "DD/MM/YYYY",
  "html.autoClosingTags": true,
  "javascript.autoClosingTags": true,
  "javascript.preferences.quoteStyle": "single",
  "less.validate": false,
  "scss.validate": false,
  "telemetry.telemetryLevel": "crash",
  "terminal.external.osxExec": "Warp.app",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "terminal.integrated.fontFamily": "'Fira Mono for Powerline'",
  "terminal.integrated.fontSize": 14,
  "terminal.integrated.fontWeight": "500",
  "terminal.integrated.tabs.enabled": true,
  "todo-tree.highlights.enabled": true,
  "typescript.autoClosingTags": true,
  "vsicons.dontShowNewVersionMessage": true,
  "window.openFilesInNewWindow": "on",
  "workbench.colorTheme": "poimandres",
  "workbench.fontAliasing": "antialiased",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.tree.indent": 12,
  "workbench.editor.empty.hint": "hidden",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "svelte.enable-ts-plugin": true,
  "cursor.cpp.disabledLanguages": ["markdown", "scminput"]
}
